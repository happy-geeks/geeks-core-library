<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <OutputType>Library</OutputType>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>3.0.7</Version>
    <Company>Happy Geeks B.V.</Company>
    <Description>Our base/core library that we use for most of our other projects.</Description>
    <AssemblyVersion>3.0.7.0</AssemblyVersion>
    <FileVersion>3.0.7.0</FileVersion>
    <PackageDescription>Geeks Core Library</PackageDescription>
    <RepositoryUrl>https://github.com/happy-geeks/geeks-core-library.git</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<Authors>Happy Geeks B.V.</Authors>
	<PackageLicenseFile>LICENSE</PackageLicenseFile>
	<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	<LangVersion>default</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Core\Cms\Services\**" />
    <Content Remove="Core\Cms\Services\**" />
    <EmbeddedResource Remove="Core\Cms\Services\**" />
    <None Remove="Core\Cms\Services\**" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Modules\PostalServices\PostNL\Models\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AspNetCore.HealthChecks.MySql" Version="6.0.2" />
    <PackageReference Include="AspNetCore.HealthChecks.System" Version="6.0.5" />
    <PackageReference Include="AspNetCore.HealthChecks.UI.Client" Version="6.0.5" />
    <PackageReference Include="BuckarooSDK" Version="1.4.1" />
    <PackageReference Include="CM.Text" Version="2.6.0" />
    <PackageReference Include="DocumentFormat.OpenXml" Version="2.19.0" />
    <PackageReference Include="EvoPdf.HtmlToPdf.NetCore.x64" Version="8.0.0" />
    <PackageReference Include="FluentFTP" Version="46.0.2" />
    <PackageReference Include="GoogleAuthenticator" Version="3.0.0" />
    <PackageReference Include="HtmlAgilityPack" Version="1.11.46" />
    <PackageReference Include="ISO3166" Version="1.0.4" />
    <PackageReference Include="ISO3166NL" Version="1.0.1" />
    <PackageReference Include="JetBrains.Annotations" Version="2022.3.1" />
    <PackageReference Include="LazyCache.AspNetCore" Version="2.4.0" />
    <PackageReference Include="libphonenumber-csharp" Version="8.13.10" />
    <PackageReference Include="Magick.NET-Q8-x64" Version="13.0.1" />
    <PackageReference Include="MailKit" Version="3.6.0" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="7.0.4" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="7.0.0" />
    <PackageReference Include="Mollie.Api" Version="2.2.0.4" />
    <PackageReference Include="MultiSafepay" Version="0.8.7" />
    <PackageReference Include="MySql.Data" Version="8.0.32.1" />
    <PackageReference Include="RestSharp" Version="109.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="RestSharp.Serializers.NewtonsoftJson" Version="109.0.1" />
    <PackageReference Include="Scrutor" Version="4.2.2" />
    <PackageReference Include="SharpZipLib" Version="1.4.2" />
    <PackageReference Include="SSH.NET" Version="2020.0.2" />
    <PackageReference Include="System.Diagnostics.Debug" Version="4.3.0" />
    <PackageReference Include="System.Globalization" Version="4.3.0" />
    <PackageReference Include="System.IO.FileSystem" Version="4.3.0" />
    <PackageReference Include="System.IO.FileSystem.Primitives" Version="4.3.0" />
    <PackageReference Include="System.Linq.Async" Version="6.0.1" />
    <PackageReference Include="System.Net.Primitives" Version="4.3.1" />
    <PackageReference Include="System.Runtime" Version="4.3.1" />
    <PackageReference Include="System.Runtime.Extensions" Version="4.3.1" />
    <PackageReference Include="System.Threading" Version="4.3.0" />
    <PackageReference Include="Twilio" Version="6.2.5" />
    <PackageReference Include="WebMarkupMin.AspNetCore3" Version="2.13.8" />
    <PackageReference Include="ZXing.Net" Version="0.16.9" />
    <PackageReference Include="ZXing.Net.Bindings.Magick" Version="0.16.13" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="OmniKassa">
      <HintPath>OmniKassa.dll</HintPath>
    </Reference>
    <Content Include="lib\OmniKassa.dll">
      <Pack>true</Pack>
      <PackagePath>lib\net7.0\</PackagePath>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <Target Name="Pack"></Target>
</Project>
